			inset-top=-1		
		}
			
		URLStatusImage {
			bgcolor=none
			inset="0 0 0 0"
			render_bg={}
			image="graphics/html_lock_disabled"
 			scaling=panel
 		}

		URLBarSecure {
			textcolor="green"
			font-size=14
		}

		URLBarBrokenSecure {
			textcolor="Red"
			font-family=semibold
		}
	
		COverlayWebBrowser {
			bgcolor=none
			render_bg {
				1="fill( x0, y0+4, x1, y0+5, darkGreyEnd )"
				2="fill( x0, y0+5, x1, y0+6, greyHighlight )"
				3="gradient( x0, y0+6, x1, y0+47, grey, lightGreyEnd )"
				4="fill( x0, y0+47, x1, y0+48, darkGreyEnd )"
				5="fill( x0, y1-21, x1, y1-20, grey )"
				6="fill( x0, y1-20, x1, y1-19, greyHighlight )"
				//7="gradient( x0, y1-19, x1, y1, grey, lightGreyEnd )"
				8="fill( x0, y1-1, x0+1, y1, darkGreyEnd )"
				9="fill( x1-1, y1-1, x1, y1, darkGreyEnd )"
			}
		}
		
		HTML {
			render {
				0="gradient( x0, y0, x1, y0+4, black75, None )"	
				1="gradient( x0, y1-5, x1, y1, None, black75 )"
			}
		}
	}

	layout {
		///// HTML Browser /////

		place { 
			control="HTMLField"   	
			width=max 
			height=max 
			margin-top=48 
			margin-left=1 
			margin-right=1
			margin-bottom=21
		}

		///// The Text down the bottom /////

		place { 
			control="StatusText"
			align=bottom 
			height=24
			margin-left=8 
			margin-bottom=-1 
		}
		
		///// Web Controls /////

		place { 
			control="URLStatusImage" 
			align=right
			margin-top=16 
			margin-right=14 
			width=20 
			height=20 
		}	
		
		place { 
			control="BackButton,ForwardButton" 
			x=7 
			y=14 
			width=28 
			height=25 
			spacing=-1 
		}
		
		place { 
			control="ReloadButton,StopButton,HomeButton" 
			x=8
			width=28 
			height=25 
			spacing=0 
			start="ForwardButton" 
		}

		place { 
			control="URLBar" 
			height=24 
			width=max 
			start="HomeButton" 
			margin-right=9 }
		
		///// Null Zone /////

		place { 
			control="EVCert,PageLoadThrobber" 
			x=0 
			y=0
			width=0
			height=0
		}
	}	
}