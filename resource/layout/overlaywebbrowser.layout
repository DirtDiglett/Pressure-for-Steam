		TextEntryURL {
			font-family=basefont
	      font-size=14
	      textcolor  = "grey"
	      bgcolor  = "none"
	      selectedtextcolor  = "white"
	      selectedbgcolor  = "blue"
	      shadowtextcolor  = "darkestGrey"  // this is the cursor color
	
	      inset-left=2
	      inset-top=0
	      
	      render_bg {
	      	0="fill(x0,y0+2,x1-2,y1-2, white)"
	
		  	// TOP
		  	2="image_tiled(x0,y0,x1-3,y0+4, graphics/textEntry/topMiddle)"
		  	3="image(x1-3,y0,x1,y0+4, graphics/textEntry/topRight)"
		  	
		  	// RIGHT
		  	5="image_tiled(x1-3,y0+4,x1,y1-3, graphics/textEntry/rightMiddle)"
		  	
		  	// BOTTOM
	      	7="image_tiled(x0,y1-2,x1-3,y1+1, graphics/textEntry/bottomMiddle)"
	      	8="image(x1-3,y1-3,x1,y1+1, graphics/textEntry/bottomRight)"
	      	
	      }
		}
		
			TextEntryURL:hover {
				
				textcolor="darkGreyEnd"
			}   			
		
			TextEntryURL:Disabled {
