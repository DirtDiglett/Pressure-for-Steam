"resource/layout/musicpage_details_playlistitem.layout" {
	styles {
		PositionLabelStyle { 
			bgcolor=none
			textcolor=lightestGrey
			font-size=16
			font-family=basefont
			font-weight=600 
			padding-top=4
			padding-bottom=4

			render_bg {
				0="fill(x0,y0-2,x1+10,y1+2, black65)"
			}				
		}
		
		TrackLabelStyle { 
			bgcolor=none
			textcolor=white
			font-size=16
			font-family=basefont
			font-weight=400 
			padding-top=4
			padding-bottom=4

			render_bg {
				0="fill(x0,y0-2,x1,y1+2, black65)"
			}
		}
		
			TrackLabelStyle:hover {
				textcolor="blue"
			}
	}
		
	layout {
		region {
			name=panelregion
			width=max
		}
		
		place {
			control="PositionLabel"
			region="panelregion"
			width=20
		}

		place {
			control="TrackLabel"
			region="panelregion"
			x=30
			width=max
		}
	}	
}