				// BOTTOM
				7="image_scale(x0,y1-2,x1,y1+1, graphics/textEntry/disabled/bottomMiddle)"

			}
		}

		"CSteamRootDialog TextEntryURL" {
			bgcolor="none"
			selectedtextcolor="white"
			selectedbgcolor="blue"
			shadowtextcolor="darkestGrey"  // this is the cursor color
			textcolor="lightestGrey"
			font-family=basefont
			font-size=15
font-size=14 [$LINUX]
