			inset-top=0

			render_bg {
				0="fill(x0,y0+2,x1,y1-2, lighterGrey)"

				 // TOP
				1="image_scale(x0,y0,x1,y0+4, graphics/textEntry/disabled/topMiddle)"

			  	// BOTTOM
