"resource/layout/uinavigatorpanel.layout" {
	styles {

		LabelEVCert {
			textcolor="darkestGrey"
			font-family=basefont
			font-size=15
			font-size=14 [$LINUX]
			font-weight=400
			inset-left=4
			inset-top=0
			inset-right=4

			render_bg {
				0="fill(x0,y0,x0+1,y1, darkestGrey)"
				1="fill(x0+1,y0,x1,y1, green)"
				2="fill(x0+1,y0,x1,y0+1, darkestGrey)"
				3="fill(x0+1,y1-1,x1,y1, darkestGrey)"
				4="fill(x0+1,y1,x1,y1+1, grey25)"
			}
		}

		URLStatusImage {
			bgcolor=none
			inset="0 0 0 0"
			image="graphics/html_lock_disabled"
			render_bg {}
		}
		URLBarSecure {
			textcolor="green"
		}
		URLBarBrokenSecure {
			textcolor="red"
		}

		// Doesn't appear to be used for anything:
		headertextlarge {
			bgcolor=green
			textcolor=green
			font-size=30
			font-weight=400
			padding-left=10
			padding-top=5
			padding-right=10
			padding-bottom=5
			minimum-width=200
		}
		LibraryShow [!$OSX] {
			textcolor="LabelDisabled"
			font-family=basefont
			font-size=15
font-size=14 [$LINUX]
			font-weight=400
			font-style=uppercase
			padding-left=0
		}
		LibraryShow [$OSX] {}

		NavLabel {
			textcolor="Label"
			font-family=basefont
			font-size=15
			font-size=14 [$LINUX]
			font-weight=400
			font-style=none
		}

		NavLabelView {
			textcolor="red"
			font-family=basefont
			font-size=15
			font-size=14 [$LINUX]
			font-weight=400
			font-style="none"
			padding-left=0
		}

		CUINavButton2 {
			bgcolor="none"
			textcolor="green"
			font-family=basefont
			font-size=15
			font-size=14 [$LINUX]
			font-weight=400
			font-style="none"
			padding-left=5
			padding-right=5
			padding-bottom=4
			render {
				0="fill(x0, y0, x1, y1, green)"
			}
		}

			CUINavButton2:hover {
				textcolor="orange"
				font-style="uppercase,outerglow"
				font-outerglow-color="TextGlowHoverSm"
				font-outerglow-offset=1
				font-outerglow-filtersize=1
			}
			CUINavButton2:selected {
				textcolor="white"
				bgcolor=none
				font-style="uppercase,outerglow"
				font-outerglow-color="TextGlowSelectedSm"
				font-outerglow-offset=1
				font-outerglow-filtersize=1
			}
		CUINavigatorPanel {
			bgcolor="none"
		}

// ********************************************************************************************************
// Control's Styles that have been 'Nulled'
// ********************************************************************************************************
		Nulled {}
		FullscreenButton {}
		LibrarySearchIcon {}
		LibrarySearchIcon:disabled {}
		CUINavButtonNew {}
		"CGamesPage_Mini CGameFilterMenuButton" {}

// ********************************************************************************************************
// Zoom
// ********************************************************************************************************
// Used as a "Zoom Out" icon
		NavLabelZoom {
			textcolor="none"
			render_bg {
				0="image(x0+14, y0+4, x1, y1, graphics/icons/zoom_out)"
			}
		}

// Used as a "Zoom In" icon
		LibrarySeparator {
			textcolor="none"
			render_bg {
				0="image(x0+4, y0+5, x1, y1, graphics/icons/zoom)"
			}
		}

		zoomslider {
			inset="0 2 0 0"
			textcolor=white
			render_bg {}
		}

			zoomslider:hover {
				textcolor=trueWhite
			}
// ********************************************************************************************************
// WebView (Pages that are website-based: Store, Community, etc.)
// ********************************************************************************************************
		WebPanelStyle {
			bgcolor="darkestGrey"
			render {
				// Create the gradient over the top.
				0="gradient(x0, y0, x1, y0+3, darkestGrey50, none)"
				1="gradient(x0, y1-3, x1, y1, none, darkestGrey50)"
			}
			render_bg {}
		}

// Background for the "Throbber" (Loading animation)
		Panel_transparent {
			bgcolor="none"
			render_bg {
				0="fill(x0,y0+2,x1-2,y1-2, lighterGrey)"

				// TOP
				1="image_scale(x0,y0,x1-3,y0+4, graphics/textEntry/disabled/topMiddle)"
				2="image(x1-3,y0,x1,y0+4, graphics/textEntry/disabled/topRight)"
				9="image(x0-3,y0,x1,y0+4, graphics/textEntry/disabled/topLeft)"

				// RIGHT
				3="image_scale(x1-3,y0+4,x1,y1-3, graphics/textEntry/disabled/rightMiddle)"
				// LEFT
				5="image_scale(x0-3,y0+4,x0,y1-3, graphics/textEntry/disabled/leftMiddle)"

				// BOTTOM
				7="image_scale(x0,y1-2,x1-3,y1+1, graphics/textEntry/disabled/bottomMiddle)"
				8="image(x1-3,y1-3,x1,y1+1, graphics/textEntry/disabled/bottomRight)"
				10="image(x0-3,y1-3,x1,y1+1, graphics/textEntry/disabled/bottomLeft)"
			}
		}
// ********************************************************************************************************
// Broadcast Page
// ********************************************************************************************************
		BroadcastPanelMinimized {
			bgcolor="none"
			textcolor="label"
			inset="0 0 0 0"

			render {
				// Create the gradient over the top.
				0="gradient(x0, y0, x1, y0+3, darkestGrey50, none)"
				1="gradient(x0, y1-3, x1, y1, none, darkestGrey50)"
			}
			render_bg {
				0="fill( x0, y0, x1, y1, almostblack )" // bg
			}
		}

// ********************************************************************************************************
// TextEntry & URL Bar
// ********************************************************************************************************
// Overrides the defaults created in the Steam.styles
		TextEntry {
			bgcolor="none"
			selectedtextcolor="white"
			selectedbgcolor="blue"
			shadowtextcolor="darkestGrey"  // this is the cursor color
			textcolor="grey"
			font-family=basefont
			font-size=15
font-size=14 [$LINUX]
			font-weight=400
			inset-left=0
			inset-top=0
			render {}
		}

			TextEntry:hover {
				textcolor="grey"
			}

			TextEntry:active {
				textcolor="darkestGrey"
			}

			TextEntry:selected {
				textcolor="darkestGrey"
			}

		TextEntryURL {
			bgcolor="none"
			selectedtextcolor="white"
			selectedbgcolor="blue"
			shadowtextcolor="darkestGrey"  // this is the cursor color
			textcolor="lightestGrey"
			font-family=basefont
			font-size=15
font-size=14 [$LINUX]
			inset-left=4
			inset-top=-1

			render_bg {
				0="fill(x0,y0+2,x1,y1-2, lighterGrey)"

				// TOP
