	layout {
		//Background Graphs (Net speed and Disk usage)
		place {
			control="net_graphpanel"
			width=max
			height=500
			margin-top=0
		}
		place {
			control="disc_graphpanel"
			width=max
			height=500
			margin-top=3
		}

		//Universal Pause/Resume Button
		place {
			control="pauseresumeallbutton"
			height=39
			align=right
			y=41
			margin-right=12
		}

		//Graph legends
		place {
			control="net_legend_panel,disk_legend_panel"
			width=15
			//height=9
		}
		place {
			control="net_legend_label,net_legend_panel,disk_legend_label,disk_legend_panel"
			align=right
			height=14
			spacing=2
			y=99
			margin-right=6
		}


		//invisible
		place {
			control="header"
			x=0
			y=-100
			height=0
			width=0
		}

		//Network Values
		place {
			control="total_downloaded_value,total_downloaded"
			x=12
			spacing=5
			height=30
			y=7
		}

		place {
			control="download_rate_value,download_rate"
			x=12
			spacing=5
			height=30
			y=32
		}

		place {
			control="peak_download_rate_value,peak_download_rate"
			x=12
			spacing=5
			height=30
			y=57
		}

		//Disk Usage
		place {
			control="disk_status_value,disk_status_label"
			x=12
			spacing=5
			height=30
			y=82
		}

		// Blackout/Throttling Values - Right
		place {
			control="blackout_label,blackout_value"
			y=7
			spacing=3
			align=right
			height=14
			margin-right=13
		}

		place {
			control="throttling_label,throttling_value"
			y=21
			spacing=3
			align=right
			height=14
			margin-right=13
		}
	}
}

