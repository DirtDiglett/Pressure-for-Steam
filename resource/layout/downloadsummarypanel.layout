			inset="0 0 0 0"
			render_bg {} // no background

			render {
				0="fill( x0, y0, x1, y1-10, black40 )"
				1="gradient( x0, y1-10, x1, y1, black40, black65 )"
				2="gradient_horizontal(x0,y0,x0+50,y1, black, none)"
			}
		}

