"original.styles" {
	styles {
		CUINavButton {
			textcolor=darkestGrey
			font-style="outerglow"
			font-outerglow-color="lightestGreyHighlight"
			font-outerglow-offset=0
			font-outerglow-filtersize=3
		}
			CUINavButton:hover {
				textcolor="white"
			}
			CUINavButton:selected {
				textcolor="trueWhite"
			}
				CUINavButton:selected:hover {
					textcolor="trueWhite"
					font-outerglow-color="lightestGrey"
					font-outerglow-offset=1
					font-outerglow-filtersize=6
				}
	}
}